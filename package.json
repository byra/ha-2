{
  "name": "with-redux",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "precommit": "jest",
    "prepush": "jest"
  },
  "jest": {
    "collectCoverageFrom": [
      "./test/*js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
      }
    },
    "reporters": [
      "default",
      [
        "./node_modules/jest-html-reporter",
        {
          "pageTitle": "Test Report"
        }
      ]
    ]
  },
  "dependencies": {
    "babel-plugin-styled-components": "^1.10.0",
    "next": "^8.1.0",
    "next-redux-wrapper": "^3.0.0-alpha.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^5.0.1",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.2",
    "styled-components": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.3.4",
    "babel-jest": "24.1.0",
    "husky": "^2.1.0",
    "jest": "24.1.0",
    "jest-html-reporter": "^2.5.0",
    "react-testing-library": "^5.4.2"
  },
  "license": "ISC"
}
